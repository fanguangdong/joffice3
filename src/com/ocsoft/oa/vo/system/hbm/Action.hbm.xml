<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" package="com.ocsoft.oa.vo.system">
	<class table="action" name="Action">
		<id name="actionId" column="action_id">
			<generator class="native" />
		</id>
		<property name="actionTypeCode" column="action_type_code" not-null="true" />
		<property name="actionTypeDesc" column="action_type_desc" not-null="true" />
		<property name="moduleId" column="module_id" not-null="true" />
	</class>
</hibernate-mapping>
