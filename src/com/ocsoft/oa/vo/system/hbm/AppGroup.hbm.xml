<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" package="com.ocsoft.oa.vo.system">
	<class table="app_group" name="AppGroup">
		<composite-id >
			<key-many-to-one name="appUser" column="user_id"></key-many-to-one>
			<key-many-to-one name="appRole" column="role_id"></key-many-to-one>
		</composite-id>
		
	</class>
</hibernate-mapping>
