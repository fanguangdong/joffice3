<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" package="com.ocsoft.oa.vo.system">
	<class table="client" name="Client">
		<id name="clientId" column="client_id">
			<generator class="native" />
		</id>
		<property name="clientName" column="client_name" not-null="true" />
		<property name="clientTitle" column="client_title" not-null="false" />
		 
	</class>
</hibernate-mapping>
